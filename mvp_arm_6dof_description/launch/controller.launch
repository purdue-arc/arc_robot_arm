<node args="Rev17_position_controller Rev20_position_controller Rev26_position_controller Rev36_position_controller Rev40_position_controller Rev45_position_controller joint_state_controller " name="controller_spawner" ns="mvp_arm_6dof" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
