<launch>
  <param command="$(find xacro)/xacro $(find arc_robot_arm_urdf_description)/urdf/arc_robot_arm_urdf.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model arc_robot_arm_urdf" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="true"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>
</launch>
